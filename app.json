{
  "expo": {
    "name": "Artemis",
    "slug": "artemis",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "artemis",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a1323"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gabrielaguiar.artemis"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#0a1323"
      },
      "package": "com.gabrielaguiar.artemis"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-sqlite"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2bb468aa-893d-4448-8274-4472d488da50"
      }
    }
  }
}
